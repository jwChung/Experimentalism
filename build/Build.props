<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003" ToolsVersion="4.0">
  <ItemGroup>
    <Projects Include="..\src\**\*.csproj" />
    <TestProjects Include="..\test\**\*.csproj" />
    <NuGetFiles Include="*.nuspec" />
    <CommonAssemblyInfo Include="CommonAssemblyInfo.cs" />
    <NuGetPath Include="..\packages\**\NuGet.exe" />
    <XunitRunnerPath Include="..\packages\**\xunit.runner.msbuild.dll" />
  </ItemGroup>
  <PropertyGroup>
    <TestAssemblies>..\test\**\bin\release\*UnitTest.dll</TestAssemblies>
    <LocalNugetFeed>D:\LocalNugetFeed</LocalNugetFeed>
    <Repository>https://$(MY_GITHUBACCOUNT)@github.com/jwChung/Experimentalism.git</Repository>
    <CurrentBranchName>$(APPVEYOR_REPO_BRANCH)</CurrentBranchName>
    <CommitMessage>$(APPVEYOR_REPO_COMMIT_MESSAGE)</CommitMessage>
    <CommitAuthor>$(APPVEYOR_REPO_COMMIT_AUTHOR)</CommitAuthor>
    <CommitAuthorEmail>$(APPVEYOR_REPO_COMMIT_AUTHOR_EMAIL)</CommitAuthorEmail>
    <SetApiKey>$(MY_SETAPIKEY)</SetApiKey>
    <PrivateKey>$(MY_PRIVATEKEY)</PrivateKey>
  </PropertyGroup>
</Project>